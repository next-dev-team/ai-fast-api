# Server settings
HOST=0.0.0.0
PORT=8000
DEBUG=true

# API settings
API_TITLE="AI FastAPI Server"
API_DESCRIPTION="OpenAI-compatible API powered by GPT4Free (G4F)"
API_VERSION=1.0.0

# G4F settings
G4F_PROVIDER=auto
G4F_MODEL=gpt-3.5-turbo
G4F_TIMEOUT=60
G4F_RETRIES=3

# Rate limiting
RATE_LIMIT_REQUESTS=600
RATE_LIMIT_WINDOW=600

# CORS settings
CORS_ENABLED=true
CORS_ORIGINS="*"

# OpenAI compatibility
OPENAI_API_BASE=/v1
# OPENAI_MODEL_MAPPING is handled internally in config.py and not directly set via env
